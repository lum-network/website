{
  "hosting": [
    {
      "target": "lum-network",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/links/**",
          "dynamicLinks": true
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "lum-network-website",
      "public": "build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/links/**",
          "dynamicLinks": true
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
